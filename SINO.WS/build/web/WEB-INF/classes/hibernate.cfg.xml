<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.OracleDialect</property>
    <property name="hibernate.connection.driver_class">oracle.jdbc.OracleDriver</property>
    <property name="hibernate.connection.url">jdbc:oracle:thin:@localhost:1521:xe</property>
    <property name="hibernate.connection.username">system</property>
    <property name="hibernate.connection.password">123456</property>
    <mapping resource="Modelo/Tstipo.hbm.xml"/>
    <mapping resource="Modelo/Tsocurraccion.hbm.xml"/>
    <mapping resource="Modelo/Tsocurrencia.hbm.xml"/>
    <mapping resource="Modelo/Tscategoria.hbm.xml"/>
    <mapping resource="Modelo/Tsestado.hbm.xml"/>
    <mapping resource="Modelo/Tsaccioargume.hbm.xml"/>
    <mapping resource="Modelo/Tssubcategori.hbm.xml"/>
    <mapping resource="Modelo/Tsnocorrespon.hbm.xml"/>
    <mapping resource="Modelo/OcurrenciaBusqueda.hbm.xml"/>
    <mapping resource="Modelo/ResumenTipo.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
